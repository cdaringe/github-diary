      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "npm-run-all": "^4.1.3",
    "prettier-standard": "^8.0.1",
    "source-map-explorer": "^1.6.0"
  },
  "browserslist": [
    ">0.25%",
    "not op_mini all"
  ]
