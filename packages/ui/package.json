{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "d3": "^5.7.0",
    "lodash": "^4.17.10",
    "panzoom": "^6.1.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-json-tree": "^0.11.0",
    "react-magic-dropzone": "^1.0.1",
    "react-scripts": "next"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "react-scripts build",
    "deploy": "run-s build scp",
    "eject": "react-scripts eject",
    "format": "prettier-standard 'src/**/*.js'",
